; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[  ]
_global_script_class_icons={

}

[application]

config/name="Mattemonster"
run/main_scene="res://Game.tscn"
boot_splash/image="res://images/splash.png"
boot_splash/bg_color=Color( 0, 0.117647, 0.231373, 1 )
config/icon="res://images/logo-large.png"

[display]

window/size/width=640
window/size/height=1024
window/handheld/orientation="portrait"
window/stretch/mode="2d"
window/stretch/aspect="keep"

[locale]

translations=PoolStringArray( "res://localization/mattemonster.en.translation", "res://localization/mattemonster.sv.translation" )

[rendering]

environment/default_clear_color=Color( 0, 0.117647, 0.231373, 1 )
environment/default_environment="res://default_env.tres"
